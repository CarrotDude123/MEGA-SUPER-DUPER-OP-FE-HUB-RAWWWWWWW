local t = game:GetService("\80\108\97\121\101\114\115") local q = game:GetService("\82\117\110\83\101\114\118\105\99\101") local n = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local i = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local _ = t.LocalPlayer local r_ = _.Character or _.CharacterAdded:Wait() local or_ = r_:WaitForChild("\72\117\109\97\110\111\105\100") local mo = r_:WaitForChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") local pm = nil local jp = {} local ej = -4  local ke = 0.5  local sk = 0.1  local fs = 100 local af = false local la = 1 local function r() if pm then pm:Destroy() end pm = Instance.new("\77\111\100\101\108") pm.Name = "\71\104\111\115\116\70\111\108\108\111\119\101\114" pm.Parent = workspace local cl = Instance.new("\80\97\114\116") cl.Name = "\84\111\114\115\111" cl.Size = Vector3.new(2, 2, 1) cl.Material = Enum.Material.Neon cl.BrickColor = BrickColor.new("\67\121\97\110") cl.Transparency = 0.3 cl.CanCollide = false cl.Anchored = true cl.Parent = pm local bc = Instance.new("\80\97\114\116") bc.Name = "\72\101\97\100" bc.Size = Vector3.new(2, 1, 1) bc.Material = Enum.Material.Neon bc.BrickColor = BrickColor.new("\67\121\97\110") bc.Transparency = 0.3 bc.CanCollide = false bc.Anchored = true bc.Parent = pm local gb = Instance.new("\80\97\114\116") gb.Name = "\76\101\102\116\32\65\114\109" gb.Size = Vector3.new(1, 2, 1) gb.Material = Enum.Material.Neon gb.BrickColor = BrickColor.new("\67\121\97\110") gb.Transparency = 0.3 gb.CanCollide = false gb.Anchored = true gb.Parent = pm local dg = Instance.new("\80\97\114\116") dg.Name = "\82\105\103\104\116\32\65\114\109" dg.Size = Vector3.new(1, 2, 1) dg.Material = Enum.Material.Neon dg.BrickColor = BrickColor.new("\67\121\97\110") dg.Transparency = 0.3 dg.CanCollide = false dg.Anchored = true dg.Parent = pm local td = Instance.new("\80\97\114\116") td.Name = "\76\101\102\116\32\76\101\103" td.Size = Vector3.new(1, 2, 1) td.Material = Enum.Material.Neon td.BrickColor = BrickColor.new("\67\121\97\110") td.Transparency = 0.3 td.CanCollide = false td.Anchored = true td.Parent = pm local qt = Instance.new("\80\97\114\116") qt.Name = "\82\105\103\104\116\32\76\101\103" qt.Size = Vector3.new(1, 2, 1) qt.Material = Enum.Material.Neon qt.BrickColor = BrickColor.new("\67\121\97\110") qt.Transparency = 0.3 qt.CanCollide = false qt.Anchored = true qt.Parent = pm pm.PrimaryPart = cl bc.CFrame = cl.CFrame * CFrame.new(0, 1.5, 0) gb.CFrame = cl.CFrame * CFrame.new(-1.5, 0.5, 0) dg.CFrame = cl.CFrame * CFrame.new(1.5, 0.5, 0) td.CFrame = cl.CFrame * CFrame.new(-0.5, -1.5, 0) qt.CFrame = cl.CFrame * CFrame.new(0.5, -1.5, 0) local nq = {cl, bc, gb, dg, td, qt} for _, part in pairs(nq) do local inq = Instance.new("\80\111\105\110\116\76\105\103\104\116") inq.Color = Color3.new(0, 1, 1) inq.Brightness = 1.5 inq.Range = 8 inq.Parent = part if part.Name == "\72\101\97\100" then local qtd = Instance.new("\83\112\101\99\105\97\108\77\101\115\104") qtd.MeshType = Enum.MeshType.Head qtd.Scale = Vector3.new(1.25, 1.25, 1.25) qtd.Parent = part end end local function o(m, p, j) local r_i = Instance.new("\87\101\108\100") r_i.Part0 = m r_i.Part1 = p r_i.C0 = j r_i.Parent = m end o(cl, bc, CFrame.new(0, 1.5, 0)) o(cl, gb, CFrame.new(-1.5, 0.5, 0)) o(cl, dg, CFrame.new(1.5, 0.5, 0)) o(cl, td, CFrame.new(-0.5, -1.5, 0)) o(cl, qt, CFrame.new(0.5, -1.5, 0)) local or_i = 0 game:GetService("\82\117\110\83\101\114\118\105\99\101").Heartbeat:Connect(function(b) if pm and pm.Parent then or_i = or_i + b * 2 local mor = math.sin(or_i) * 0.3 local pmo = math.sin(or_i * 0.5) * 5 for _, part in pairs(nq) do if part and part.Parent == pm then local ske = part.CFrame part.CFrame = ske * CFrame.new(0, mor * b, 0) * CFrame.Angles(0, math.rad(pmo * b), 0) end end end end) end local function e() if not pm then return end local ejp = mo.CFrame:VectorToWorldSpace(Vector3.new(0, 0, -ej)) local kej = mo.Position + ejp + Vector3.new(0, ke, 0) local ske = pm:GetPrimaryPartCFrame() local fsk = CFrame.new(kej, mo.Position) local afs = ske:Lerp(fsk, sk) pm:SetPrimaryPartCFrame(afs) for _, part in pairs(pm:GetChildren()) do if part:IsA("\66\97\115\101\80\97\114\116") then part.CanCollide = false part.Anchored = true end end end local function k() if af then return end local laf = _:GetMouse() if not laf.Hit then return end local cla = laf.Hit.Position local bcl = (cla - mo.Position).Magnitude local gbc if bcl <= fs then gbc = cla else local dgb = (cla - mo.Position).unit gbc = mo.Position + (dgb * fs) end local function s(f, a) local tdg = Instance.new("\80\97\114\116") tdg.Size = Vector3.new(1, 1, 1) tdg.Position = f tdg.Anchored = true tdg.CanCollide = false tdg.Transparency = 0 tdg.BrickColor = a tdg.Material = Enum.Material.Neon tdg.Parent = workspace local qtd = Instance.new("\83\112\101\99\105\97\108\77\101\115\104") qtd.MeshType = Enum.MeshType.Sphere qtd.Scale = Vector3.new(0.1, 0.1, 0.1) qtd.Parent = tdg local nqt = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) local inqt = i:Create(qtd, nqt, {Scale = Vector3.new(10, 10, 10)}) local _i = i:Create(tdg, nqt, {Transparency = 1}) inqt:Play() _i:Play() _i.Completed:Connect(function() tdg:Destroy() end) end s(mo.Position, BrickColor.new("\67\121\97\110")) s(gbc, BrickColor.new("\87\104\105\116\101")) mo.CFrame = CFrame.new(gbc) af = true task.wait(la) af = false end local function l(c) r_ = c or_ = r_:WaitForChild("\72\117\109\97\110\111\105\100") mo = r_:WaitForChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") task.wait(0.1) r() end n.InputBegan:Connect(function(g, d) if d then return end if g.KeyCode == Enum.KeyCode.K then k() end end) q.Heartbeat:Connect(function() e() end) r() _.CharacterAdded:Connect(l)
