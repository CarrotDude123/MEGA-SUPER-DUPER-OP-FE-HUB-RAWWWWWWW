end)
    function addMenuButton(name, text, layoutOrder, url, successMsg, errorMsg)
	local button = Instance.new("TextButton")
	button.Name = name
	button.Text = text
	button.Font = Enum.Font.SourceSansBold
	button.TextSize = 18
	button.TextColor3 = Color3.new(0,0,0)
	button.BackgroundColor3 = Color3.new(1,1,1)
	button.BorderSizePixel = 1
	button.Size = UDim2.new(1, 0, 0, 40)
	button.LayoutOrder = layoutOrder
	button.Parent = scrollFrame
	local clickSound = createClickSound()
	clickSound.Parent = button
	button.MouseButton1Click:Connect(function()
		clickSound:Play()
		if not game.HttpGet or not loadstring then
			outputLabel.Text = "Error: game:HttpGet or loadstring is not available in this environment."
			return
		end
		local code
		local success, msg = pcall(function()
		code = game:HttpGet(url)
		end)
		if success and code then
			local func, err = loadstring(code)
			if func then
				local ok, result = pcall(func)
				if ok then
					outputLabel.Text = successMsg
				else
					outputLabel.Text = errorMsg..": "..tostring(result)
				end
			else
				outputLabel.Text = errorMsg.." (compile): "..tostring(err)
			end
		else
			outputLabel.Text = errorMsg.." (fetch): "..tostring(msg)
		end
	end)
	return button
end
