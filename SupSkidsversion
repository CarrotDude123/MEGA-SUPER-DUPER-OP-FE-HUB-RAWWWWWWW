----
cmdBar:GetPropertyChangedSignal("Text"):Connect(function()
	local lineCount = select(2, cmdBar.Text:gsub("\n", "\n")) + 1
	local neededHeight = math.max(200, lineCount * 22)
	cmdBar.Size = UDim2.new(1, -10, 0, neededHeight)
	cmdBarScroll.CanvasSize = UDim2.new(0, 0, 0, neededHeight)
end)
