local function createClickSound()
	local sound = Instance.new("Sound")
	sound.Name = "clicksound"
	sound.SoundId = "rbxassetid://9118823104" -- sound but not working ¯\_(ツ)_/¯ u can fix this if you want
	sound.Volume = 0.5
	sound.PlayOnRemove = false
	sound.Looped = false
	sound.Parent = nil 
	return sound
end
