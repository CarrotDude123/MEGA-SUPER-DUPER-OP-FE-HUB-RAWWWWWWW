runButton.MouseButton1Click:Connect(function()
	runClickSound:Play()
	local code = cmdBar.Text
	if code and code ~= "" then
		if not loadstring then
			outputLabel.Text = "Error: loadstring is not available in this environment."
			return
		end
		local func, err = loadstring(code)
		if func then
			local success, msg = pcall(func)
			if not success then
				outputLabel.Text = "Error: "..tostring(msg)
			else
				outputLabel.Text = "Script ran successfully."
			end
		else
			outputLabel.Text = "Compile error: "..tostring(err)
		end
	else
		outputLabel.Text = "Please enter code to run."
	end
end)
wsButton.MouseButton1Click:Connect(function()
	wsClickSound:Play()
	local character = LocalPlayer.Character
	if character then
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			local inputText = wsInput.Text
			local newSpeed = tonumber(inputText)
			if newSpeed and newSpeed > 0 then
				humanoid.WalkSpeed = newSpeed
				outputLabel.Text = "WalkSpeed set to "..newSpeed
			else
				outputLabel.Text = "Please enter a valid WalkSpeed number."
			end
		else
			outputLabel.Text = "No Humanoid found."
		end
	else
		outputLabel.Text = "No character found."
	end
end)
jpButton.MouseButton1Click:Connect(function()
	jpClickSound:Play()
	local character = LocalPlayer.Character
	if character then
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			local inputText = jpInput.Text
			local newJP = tonumber(inputText)
			if newJP and newJP > 0 then
				outputLabel.Text = "JumpPower set to "..newJP
				humanoid.JumpPower = newJP
			else
				outputLabel.Text = "Please enter a valid JumpPower number."
			end
		else
			outputLabel.Text = "No Humanoid found."
		end
	else
		outputLabel.Text = "No character found."
	end
end)
